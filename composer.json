{
    "name": "m6web/wsclient-bundle",
    "type": "symfony-bundle",
    "description" : "Simple webservices client to call external urls",
    "authors": [
        {
            "name": "Florent DUBOST"
        },
        {
            "name": "Olivier Mansour"
        }
    ],
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "symfony/stopwatch" : "@stable",
        "guzzlehttp/cache-subscriber": "@stable"
    },
    "require-dev": {
        "atoum/atoum": "~1.0",
        "atoum/atoum-bundle": "~1.0",
        "guzzlehttp/guzzle" : "@stable",
        "m6web/coke": "~1.2",
        "m6web/symfony2-coding-standard": "~2.0",
        "symfony/symfony"   : "~2.7"
    },
    "config": {
        "bin-dir": "bin"
    },
    "suggest": {
        "m6web/redis-bundle": "@stable"
    }
}
