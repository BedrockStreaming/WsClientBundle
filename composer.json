{
    "name": "m6web/wsclient-bundle",
    "type": "symfony-bundle",
    "description" : "Simple webservices client to call external urls",
    "authors": [
        {
            "name": "Florent DUBOST"
        },
        {
            "name": "Olivier Mansour"
        }
    ],
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "minimum-stability" : "dev",
